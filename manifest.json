{
	"name": "LessonBot",
	"version": "1.0",
	"description": "超星网课小工具",
	"permissions": ["storage"],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"content_scripts": [
		{
			"matches": ["https://*.chaoxing.com/ananas/modules/video/index.html?*"],
			"run_at": "document_end",
			"js": ["videoScript.js"],
			"all_frames": true
		}
	],
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "images/LessonBot16.png",
			"32": "images/LessonBot32.png",
			"48": "images/LessonBot48.png",
			"128": "images/LessonBot128.png"
		}
	},
	"icons": {
		"16": "images/LessonBot16.png",
		"32": "images/LessonBot32.png",
		"48": "images/LessonBot48.png",
		"128": "images/LessonBot128.png"
	},
	"manifest_version": 2
}